{
  "name": "lama-express",
  "description": "Lama Project API with Express.js",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "npm-run-all clean transpile",
    "clean": "rimraf dist",
    "transpile": "babel ./src --out-dir dist",
    "start": "node ./dist/index.js",
    "prod": "npm-run-all build start",
    "dev": "babel-node src/index.js",
    "watch": "nodemon --exec yarn dev",
    "test": "jest",
    "knex": "babel-node -r dotenv/config node_modules/knex/bin/cli.js",
    "knex:migrate": "yarn knex migrate:latest",
    "knex:seed": "yarn knex seed:run",
    "lint": "node_modules/.bin/eslint --cache --ext .js ./src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "pre-commit": ["lint"],
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "auth0": "^2.30.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "http-errors": "^1.8.0",
    "jwks-rsa": "^1.11.0",
    "knex": "^0.21.12",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "objection": "^2.2.3",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.7",
    "@jest/globals": "^26.6.2",
    "babel-jest": "^26.6.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "yarn-run-all": "^3.1.1"
  }
}
